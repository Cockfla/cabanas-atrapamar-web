---
import Header from "../components/Header.astro";
import Layout from "../layouts/Layout.astro";
import Main from "../components/Main.astro";
import "../styles/global.css";
import Tarifas from "../components/Tarifas.astro";
import Ubicacion from "../components/Ubicacion.astro";
import Servicios from "../components/Servicios.astro";
import Contacto from "../components/Contacto.astro";
import InstagramFeed from "../components/Instagram-Feed.astro";
import Reglamento from "../components/Reglamento.astro";
import DisponibilidadCabañas from "../components/DisponibilidadCabañas.jsx";

export const prerender = false;
---

<Layout>
  <main>
    <Header />
    <Main />
    <Tarifas />
    <Servicios />
    <Ubicacion />
    <DisponibilidadCabañas client:load />
    <Contacto />
    <InstagramFeed />
    <Reglamento />
  </main>
</Layout>
